<view class="">
  <view  wx:if="{{!isLogin}}" style="height: 500rpx;">
    <view class="displayFlex userTop colorFFF">
        <view class="userView">
          <view class="userImg">
            <open-data  type="userAvatarUrl"></open-data>
          </view>
        </view>
        <view class="displayGrid">
          <view class="fs35 textLeft">{{userInfo.uname}}SKY</view>
          <view class="fs30"><i class="iconfont icon-shouji color8c"></i><text class="ml10">{{userInfo.mobile}}18121118073</text></view>
        </view>
      </view>
    <view class="displayFlex orderView">
      <view catchtap="to_userOrder" class="flex1">
        <view><i class="fs55 iconfont icon-icon2-copy color59 "></i></view>
        <view class="mt10 fs20">我的订单</view>
      </view>
      <view catchtap="to_historyOrder" class="flex1">
        <view><i class="fs55 iconfont icon-lishidingdan color59 "></i></view>
        <view class="mt10 fs20">历史订单</view>
      </view>
    </view>
  </view>
  <view wx:if="{{isLogin}}">
      <view>
        <button open-type="getPhoneNumber" bindgetphonenumber="getPhoneNumber" class="btnTxt1">
          <text class="fs30" style="color:#2E98FF;">我要出租</text>
        </button>
      </view>
      <view>
        <button open-type="getPhoneNumber" bindgetphonenumber="getPhoneNumber" class="btnTxt2">
          <text class="fs30" style="color:#2E98FF;">我要租车</text>
        </button>
      </view>
  </view>
