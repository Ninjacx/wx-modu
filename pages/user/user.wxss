
.btnTxt1{
  border: 15rpx solid #ff8d00;
  padding: 18rpx;
  border-radius: 20rpx;
  width: 600rpx;
}
.btnTxt2{
  border: 15rpx solid green;
  padding: 18rpx;
  border-radius: 20rpx;
  width: 600rpx;
  margin-top: 30rpx;
}
.topView{
  text-align: center;
  padding-top: 182rpx;
}
.topIcon{
  font-size: 144rpx;
  position: absolute;
  top: 0; 
  left: 0; 
  right: 0;
}
.userView{
  margin: auto 0;
}
.userImg{
  overflow: hidden;
  display: block;
  width: 120rpx;
  height: 120rpx;
  margin-right: 30rpx;
  border-radius: 100rpx;
  border: 5rpx solid #fbd388e3;
}
.userTop{
  background: linear-gradient(180deg, #1f1f1f 0%, #6a7377 100%);
  padding: 110rpx 0 150rpx 40rpx;
  box-shadow: 0px 8rpx 16rpx 0 #F5F5F5, 0 0 20rpx 0 #E8E8E8;
}
.orderView{
  text-align: center;
  background: #ffffff;
  margin: 0 38rpx;
  padding: 35rpx 0;
  border-radius: 10rpx;
  position: relative;
  top: -70rpx;
  box-shadow: 0 10rpx 6rpx 0 #F5F5F5, 0 0 10rpx -4rpx #000000;
}
.memberBottom{
  padding: 40rpx 0;
  border-top: 2rpx solid #E8E8E8;
  margin-top: 59rpx;
}
.callPhone{
  background: #98948c;
  padding: 5rpx 10rpx;
  border-radius: 10rpx;
  color: #ffffff;
  font-size: 26rpx;
  margin-left: 13rpx;
}
.cardView{
  border-bottom: 2rpx solid #dedede;
  margin: 30rpx 0;
}
.userMenuView{
  position: absolute;
  top: 113rpx;
  width: 100%;
}
.loginView{
  margin: 0 auto;
  position: relative;
  top: 40%;
  transform: translateY(-50%);
}
.bottomView{
  text-align: center;
  margin-bottom: 20rpx;
  position: absolute;
  bottom: 0;
  left: 0;
  right: 0;
}